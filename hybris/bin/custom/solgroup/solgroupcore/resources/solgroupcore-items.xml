	<relations>
		<relation code="CMSSite2CountryRelation" generate="true"
				  localized="false" autocreate="true">
			<description>This relation links CMSSite to countries</description>
			<sourceElement qualifier="cmsSite" type="CMSSite"
						   cardinality="one">
				<modifiers read="true" write="true" search="true"
						   optional="true" />
			</sourceElement>
			<targetElement qualifier="countries" type="Country"
						   cardinality="many" collectiontype="set">
				<modifiers read="true" write="true" search="true"
						   optional="true" />
			</targetElement>
		</relation>


		<relation code="CMSSite2LegacySystemRelation" generate="true"
				  localized="false" autocreate="true">
			<description>This relation links CMSSites to legacy systems</description>
			<deployment table="tblSite2Legacy" typecode="12007" />
			<sourceElement qualifier="cmsSites" type="CMSSite"
						   cardinality="many" collectiontype="set">
				<modifiers read="true" write="true" search="true"
						   optional="true" />
			</sourceElement>
			<targetElement qualifier="legacySystems" type="LegacySystem"
						   cardinality="many" collectiontype="set">
				<modifiers read="true" write="true" search="true"
						   optional="true" />
			</targetElement>
		</relation>

		<!--relation between order and sub order 1-n -->
		<relation code="Order2SubOrdersRelation" generate="true"
				  localized="false" autocreate="true">
			<description>This relation links Order to SubOrders</description>
			<sourceElement type="Order" cardinality="one"
						   qualifier="order">
				<modifiers read="true" write="true" search="true"
						   optional="false" />
			</sourceElement>
			<targetElement type="SubOrder" cardinality="many"
						   qualifier="subOrders">
				<modifiers read="true" write="true" search="true"
						   optional="true" partof="true" />
			</targetElement>
		</relation>

		<!--relation between subOrder and subOrderEntry 1-n -->
		<relation code="SubOrder2SubOrderEntryRelation" generate="true"
				  localized="false" autocreate="true">
			<description>This relation links SubOrder to SubOrderEntrys</description>
			<sourceElement type="SubOrder" cardinality="one"
						   qualifier="subOrder">
				<modifiers read="true" write="true" search="true"
						   optional="false" />
			</sourceElement>
			<targetElement type="SubOrderEntry" cardinality="many"
						   qualifier="subOrderEntry">
				<modifiers read="true" write="true" search="true"
						   optional="true" partof="true" />
			</targetElement>
		</relation>

